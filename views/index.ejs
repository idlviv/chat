<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1><%= title %></h1>
  <p>Welcome to <%= title %></p>
  <script src="/socket.io/socket.io.js"></script>
  <script>
    let socket = io();
    socket.on('news', (data) => {
      console.log('news', data);
      socket.emit('myEvent', 'news received');

    });
  </script>
</body>

</html>